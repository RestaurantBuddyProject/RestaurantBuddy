            <li id="bar_stool"><a href="#">Bar Stool</a></li>
        </ul>
    </div>
    <a href="#revealPanel" class="ui-btn ui-btn-inline ui-corner-all ui-shadow">Waiting Guests</a>

<div data-role="panel" id="revealPanel" data-position="right" data-display="reveal">
    <h2>Waiting Guests</h2>
    <ul class="list-group">
        <li class="list-group-item">Smith - Party of 4 - Booth</li>
        <li class="list-group-item">Jones - Part of 2 - Table</li>
        <li class="list-group-item">Johnson - Party of 6 - Table</li>
    </ul>
    <a href="#pageone" data-rel="close" class="ui-btn ui-btn-inline ui-shadow ui-corner-all ui-btn-a ui-icon-delete ui-btn-icon-right">Close</a>
</div>
<div id="pageWrap">
    <div id="dragElement" style="position: absolute; max-height: 200px; margin-bottom: 80%;"></div>
</div>
    <script>
        jQuery.fn.center = function () {
            this.css("position", "absolute");
            this.css("top", Math.max(0, (($(window).height() - $(this).outerHeight()) / 2) +
                                                        $(window).scrollTop()) + "px");
            this.css("left", Math.max(0, (($(window).width() - $(this).outerWidth()) / 2) +
                                                        $(window).scrollLeft()) + "px");
            return this;
        }
        $('#table2_rectangular').click(function () {
